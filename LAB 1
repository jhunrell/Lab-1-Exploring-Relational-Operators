
#include <iostream>
using namespace std;
int main() {

    int a = 5, b = 10, c = 5;

    cout << "a == b: " << (a == b) << endl;
    cout << "a != c: " << (a != c) << endl;
    cout << "a < b: " << (a < b) << endl;
    cout << "b > c: " << (b > c) << endl;
    cout << "(a + c) <= b: " << ((a + c) <= b) << endl;
 
    cout << "'A' < 'a': " << ('A' < 'a') << endl;
   
    cout << "Uy, 'A' < 'a' ay true kasi 'A' (ASCII 65) ay nasa unahan ng 'a' (ASCII 97) sa ASCII table. Ganyan talaga sa comparison!" << endl;
    return 0;
}
